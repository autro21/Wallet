<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.hesham.wallet">

	<application
			android:allowBackup="true"
			android:icon="@drawable/walet"
			android:label="@string/app_name"
			android:roundIcon="@mipmap/ic_launcher_round"
			android:supportsRtl="true"
			android:theme="@style/AppTheme">
		<activity android:name=".ToDoTaskActivity" android:theme="@style/TaskStyle">
		</activity>
		<activity
				android:name=".ToDoActivity"
				android:theme="@style/TodoStyle">
		</activity>
		<activity android:name=".MainActivity" android:theme="@style/mainOnly">
			<intent-filter>
				<action android:name="android.intent.action.MAIN"/>

				<category android:name="android.intent.category.LAUNCHER"/>
			</intent-filter>
		</activity>
	</application>

</manifest>